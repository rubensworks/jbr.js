{
  "name": "@jbr-experiment/ldbc-snb-decentralized",
  "version": "0.0.1",
  "description": "LDBC SNB Decentralized experiment handler for JBR",
  "main": "index.js",
  "typings": "index",
  "lsd:module": true,
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubensworks/jbr.js.git",
    "directory": "packages/experiment-ldbc-snb-decentralized"
  },
  "bugs": {
    "url": "https://github.com/rubensworks/jbr.js/issues"
  },
  "homepage": "https://github.com/rubensworks/jbr.js#readme",
  "license": "MIT",
  "keywords": [
    "experiment",
    "LDBC SNB Decentralized",
    "jbr",
    "benchmarking"
  ],
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/templates",
    "index.d.ts",
    "index.ts",
    "components"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:components": "componentsjs-generator -s lib --typeScopedContexts -i ../../.componentsignore",
    "validate": "npm ls"
  },
  "devDependencies": {
    "jbr": "^0.0.1"
  },
  "peerDependencies": {
    "jbr": ">=0.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/yargs": "^17.0.0",
    "fs-extra": "^10.0.0",
    "ldbc-snb-decentralized": "^1.0.1",
    "sparql-benchmark-runner": "^2.1.0"
  }
}
